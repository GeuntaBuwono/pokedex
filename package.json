{
  "name": "pokedex",
  "version": "0.0.1",
  "engines": {
    "node": "v18",
    "yarn": "v1",
    "npm": "v9"
  },
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "watch:test": "jest --watch --config ./jest.config.ts",
    "test": "jest --config ./jest.config.ts",
    "test:clear": "jest --config ./jest.config.ts --clearCache",
    "test:update": "jest -u --config ./jest.config.ts",
    "test:coverage": "jest --coverage --config ./jest.config.ts",
    "check:lint": "eslint --ignore-path .gitignore .",
    "check:prettier": "prettier --config .prettierrc.js ./src/ --check",
    "check:ts": "tsc --noEmit",
    "checker": "yarn check:lint && yarn check:prettier && yarn tsc && yarn test",
    "format:lint": "eslint --fix ./src/",
    "format:prettier": "prettier --config .prettierrc.js ./src/ --write",
    "formatter": "yarn format:lint && yarn format:prettier"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "jest": "^29.3.1",
    "metro-react-native-babel-preset": "0.74.1",
    "prettier": "^2.8.3",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
